	a:link 
	{
		text-decoration:none;
	}
	
	a:visited 
	{
		text-decoration:none;
	}
	
	h1
	{
		font-size:1.5em;
	}
	
	#Container
	{
		font-family:"Verdana", Arial, Helvetica;
		font-size:1em;
	}

	#Notification
	{
		border:2px solid #507AAA;
		background-color:#F0F0F0;
		font-size:1.2em;
		width:75%;

	}
	
	#UploadButton
	{
		text-align:right;
	}
	
	#FileTable
	{
		width:75%;
		border:2px solid #507AAA;
	}
	
	#FileTable td, #FileTable th
	{
		font-family:"Trebuchet MS", Arial, Helvetica, sans-serif;
		border-collapse:collapse;
		font-size:1em;
		border:2px solid #507AAA;
		padding:3px 7px 2px 7px;		
	}
	
	#FileTable th
	{
		font-size:1em;
		text-align:left;
		padding-top:5px;
		padding-bottom:4px;
		background-color:#507AAA;
		color:#ffffff;	
	}
	
	#FileTable tr.alt td
	{
		color:#000000;
		background-color:#F0F0F0;
	}
	
	#LeftBox
	{
		float: left;
	}
	
    #odd {background-color:#f6f7f8;}
    #even {background-color: #fff;}
	
	
	